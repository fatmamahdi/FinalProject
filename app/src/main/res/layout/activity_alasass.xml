  <TextView
      android:id="@+id/textView5"
      android:layout_width="343dp"
      android:layout_height="98dp"
      android:text="اهلا صناع المستقبل"
      android:textColor="#FFF9C9"
      android:textColorHint="#EFE0E0"
      android:textColorLink="#F1E7E7"
      android:textSize="48sp"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.493"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.044" />

  <TextView
      android:id="@+id/textView6"
      android:layout_width="197dp"
      android:layout_height="42dp"
      android:text="هيا لننطلق بكل حماس"
      android:textColor="#F8DF05"
      android:textSize="24sp"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.498"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.15" />

  <ImageView
      android:id="@+id/imageView7"
      android:layout_width="231dp"
      android:layout_height="110dp"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.48"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.716"
      app:srcCompat="@drawable/ic_baseline_videogame_asset_24" />

  <TextView
      android:id="@+id/textView7"
      android:layout_width="210dp"
      android:layout_height="58dp"
      android:text="فيديوهات الدوره"
      android:textAlignment="center"
      android:textColor="#0C0000"
      android:textSize="28sp"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.497"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.464" />

  <TextView
      android:id="@+id/textView8"
      android:layout_width="167dp"
      android:layout_height="46dp"
      android:text="الالعاب"
      android:textAlignment="center"
      android:textColor="#130101"
      android:textSize="28sp"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.849" />

</androidx.constraintlayout.widget.ConstraintLayout>